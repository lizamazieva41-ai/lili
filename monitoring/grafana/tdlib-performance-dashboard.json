{
  "dashboard": {
    "title": "TDLib Performance",
    "tags": ["tdlib", "performance", "telegram"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "rate(tdlib_requests_total[5m])",
            "legendFormat": "{{method}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Requests/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 2,
        "title": "Request Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(tdlib_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(tdlib_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(tdlib_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Latency" },
          { "format": "short" }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(tdlib_errors_total[5m])",
            "legendFormat": "{{error_type}} ({{error_code}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Errors/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 4,
        "title": "Queue Depth",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "tdlib_queue_depth",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Jobs" },
          { "format": "short" }
        ]
      },
      {
        "id": 5,
        "title": "Active Clients Over Time",
        "type": "graph",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "tdlib_active_clients",
            "legendFormat": "Active Clients",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Clients" },
          { "format": "short" }
        ]
      }
    ]
  }
}
